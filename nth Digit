import numpy as np


def solution(temp1, temp2):
    temp3 = []

    for i in temp2:
        if i not in temp1:
            temp3.append(i)

    result = []

    for i in temp3:
        if i not in result:
            result.append(i)
    return result


#temp1 = ["abc", "hq", "xyz"]
#temp2 = ["hq", "abc", "pp", "xy", "pp", "hq"]

temp1 = ["world", "prog"]
temp2 = ["hello", "world", "code", "hello", "try", "code"]

result = solution(temp1, temp2)

print(result)






import numpy as np
