temp2 = ["hello", "world", "code", "hello", "try", "code"]
temp3 = []

for i in temp2:
    if i not in temp1:
        temp3.append(i)
print(temp3)

result = []
for i in temp3:
    if i not in result:
        result.append(i)
print(result)








def NthCharacter(n):
    s = ""
    c = 1

    while(True):

        if (c < 10):
            s += chr(48 + c)

        else:
            s1 = ""
            dup = c

            while (dup > 0):
                s1 += chr((dup % 10) + 48)
                dup //= 10

            s1 = "".join(reversed(s1))

            s += s1
        c += 1

        if (len(s) >= n):
            return s[n - 1]


# Driver Code
if __name__ == "__main__":

    n = 13
    print(NthCharacter(n))

